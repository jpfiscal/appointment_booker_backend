{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "email",
        "password"
    ],
    "properties": {
        "email": {
            "type": "string",
            "default": "",
            "max": 254,
            "format": "email",
            "title": "The Email Schema",
            "examples": [
                "hello@hey.com"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "max": 254,
            "title": "The Password Schema",
            "examples": [
                "password123"
            ]
        }
    },
    "examples": [{
        "email": "hey@hello.com",
        "password": "Pa55w0rd_123"
    }]
}