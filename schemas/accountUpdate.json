{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [],
    "properties": {
        "name": {
            "type": "string",
            "title": "The Name Schema",
            "examples": [
                "Brother Mouson", "Skip Marouch", "Jane Doe"
            ]
        },
        "password": {
            "type": "string",
            "title": "The Password Schema",
            "examples": [
                "Password543"
            ]
        },
        "email": {
            "type": "string",
            "format": "email",
            "title": "The Email Schema",
            "examples": [
                "hello@hey.com"
            ]
        },
        "phone": {
            "type": "string",
            "pattern": "^(\\([0-9]{3}\\))?[0-9]{3}-[0-9]{4}$",
            "title": "The Phone Schema",
            "examples": [
                "(588)-858-0000"
            ]
        }
    },
    "examples": [
        {},
        {
            "name": "Joe Malone",
            "password": "Password123",
            "email": "holla@me.com",
            "phone": "(281) 330-8004"
        }
    ]
}